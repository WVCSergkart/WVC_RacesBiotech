	<biostatArc>1</biostatArc>
	<biostatMet>-2</biostatMet>
	<customEffectDescriptions>
		<li>&lt;color=#8a8ae6&gt;Shapeshifter&lt;/color&gt; will not remove the &lt;color=#8a8ae6&gt;archiver&lt;/color&gt; gene when used.</li>
	</customEffectDescriptions>
	<modExtensions>
		<!-- <li Class="WVC_XenotypesAndGenes.GeneExtension_Undead"> -->
			<!-- <reqGeneMat>77</reqGeneMat> -->
		<!-- </li> -->
		<li Class="WVC_XenotypesAndGenes.GeneExtension_Giver">
			<geneDefs>
				<li>WVC_Archiver</li>
			</geneDefs>
		</li>
	</modExtensions>
  </GeneDef>

</Defs>