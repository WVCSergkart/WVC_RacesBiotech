  <RulePackDef>
	<defName>WVC_NamerGenepack</defName>
	<rulePack>
	  <rulesStrings>
		<li>r_name->w-pack ([geneWord][otherGenesInfo])</li>

		<li>otherGenesInfo(geneCount==1)-></li>
		<li>otherGenesInfo(geneCount>=2)-> +[geneCountMinusOne]</li>
	  </rulesStrings>
	</rulePack>
  </RulePackDef>

