<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <HediffDef Name="WVC_XenotypesAndGenes_Hediffs_Base" Abstract="True">
	<label>test</label>
	<description>Should be invisible.</description>
	<defaultLabelColor>(255,255,255)</defaultLabelColor>
	<hediffClass>HediffWithComps</hediffClass>
	<scenarioCanAdd>false</scenarioCanAdd>
	<isBad>false</isBad>
	<everCurableByItem>false</everCurableByItem>
	<makesAlert>false</makesAlert>
	<keepOnBodyPartRestoration>true</keepOnBodyPartRestoration>
	<countsAsAddedPartOrImplant>false</countsAsAddedPartOrImplant>
	<allowMothballIfLowPriorityWorldPawn>true</allowMothballIfLowPriorityWorldPawn>
  </HediffDef>

  <!--=====================================================================-->
  
  <HediffDef ParentName="WVC_XenotypesAndGenes_Hediffs_Base">
	<defName>WVC_RB_Mecha_ScytheTail</defName>
	<label>scythe tail</label>
	<stages>
	  <li>
		<!-- <partIgnoreMissingHP>true</partIgnoreMissingHP> -->
		<becomeVisible>false</becomeVisible>
	  </li>
	</stages>
	<comps>
	  <li Class="HediffCompProperties_VerbGiver">
		<tools>
		  <li>
			<label>scythe tail</label>
			<capacities>
			  <li>Cut</li>
			  <li>Stab</li>
			</capacities>
			<power>20</power>
			<cooldownTime>1.5</cooldownTime>
			<armorPenetration>0.8</armorPenetration>
			<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
			<soundMeleeHit>Pawn_Melee_MechanoidSlash_HitPawn</soundMeleeHit>
			<soundMeleeMiss>Pawn_Melee_MechanoidSlash_Miss</soundMeleeMiss>
			<surpriseAttack>
			  <extraMeleeDamages>
				<li>
				  <def>Stun</def>
				  <amount>14</amount>
				</li>
			  </extraMeleeDamages>
			</surpriseAttack>
		  </li>
		</tools>
	  </li>
	</comps>
  </HediffDef>

  <HediffDef ParentName="WVC_XenotypesAndGenes_Hediffs_Base">
	<defName>WVC_RB_MechaClaws</defName>
	<label>foldable claw</label>
	<description>Deadly sharp claw.</description>
	<hediffClass>HediffWithComps</hediffClass>
	<addedPartProps>
	  <!-- <solid>true</solid> -->
	  <partEfficiency>1.0</partEfficiency>
	  <betterThanNatural>true</betterThanNatural>
	</addedPartProps>
	<comps>
	  <li Class="HediffCompProperties_VerbGiver">
		<tools>
		  <li>
			<label>claw</label>
			<capacities>
			  <li>Cut</li>
			  <li>Stab</li>
			</capacities>
			<power>20</power>
			<cooldownTime>1.6</cooldownTime>
			<armorPenetration>0.8</armorPenetration>
			<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
			<soundMeleeHit>Pawn_Melee_MechanoidSlash_HitPawn</soundMeleeHit>
			<soundMeleeMiss>Pawn_Melee_MechanoidSlash_Miss</soundMeleeMiss>
			<surpriseAttack>
			  <extraMeleeDamages>
				<li>
				  <def>Stun</def>
				  <amount>14</amount>
				</li>
			  </extraMeleeDamages>
			</surpriseAttack>
		  </li>
		</tools>
	  </li>
	</comps>
  </HediffDef>

  <HediffDef ParentName="WVC_XenotypesAndGenes_Hediffs_Base">
	<defName>WVC_RB_NaturalClaws</defName>
	<label>claw</label>
	<description>Deadly sharp claw. Not very suitable for fine work.</description>
	<hediffClass>HediffWithComps</hediffClass>
	<addedPartProps>
	  <!-- <solid>true</solid> -->
	  <partEfficiency>0.80</partEfficiency>
	  <betterThanNatural>true</betterThanNatural>
	</addedPartProps>
	<comps>
	  <li Class="HediffCompProperties_VerbGiver">
		<tools>
		  <li>
			<label>claw</label>
			<capacities>
			  <li>Cut</li>
			  <li>Stab</li>
			</capacities>
			<power>18</power>
			<cooldownTime>1.6</cooldownTime>
			<armorPenetration>0.4</armorPenetration>
			<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
			<surpriseAttack>
			  <extraMeleeDamages>
				<li>
				  <def>Stun</def>
				  <amount>14</amount>
				</li>
			  </extraMeleeDamages>
			</surpriseAttack>
		  </li>
		</tools>
	  </li>
	</comps>
  </HediffDef>

