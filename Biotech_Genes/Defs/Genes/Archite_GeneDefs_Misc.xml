  <GeneDef ParentName="WVC_MechanoidizationBase">
	<defName>WVC_MechaHidden_ArchiteForge</defName>
	<label>self-replicator</label>
	<!-- <label>archite-forge</label> -->
	<description>Carriers of this gene have archites that are capable of self-replication. Thanks to this, archites from time to time restore lost limbs or heals scars.</description>
	<!-- <selectionWeight>0.01</selectionWeight> -->
	<geneClass>WVC.Gene_RestoreMissingBodyParts</geneClass>
	<iconColor>(184, 198, 203)</iconColor>
	<marketValueFactor>5</marketValueFactor>
	<iconPath>WVC/UI/Genes/Gear</iconPath>
	<displayCategory>Archite</displayCategory>
	<displayOrderInCategory>0</displayOrderInCategory>
	<!-- <prerequisite>WVC_MechaSkin</prerequisite> -->
	<biostatCpx>4</biostatCpx>
	<biostatArc>2</biostatArc>
	<biostatMet>0</biostatMet>
	<!-- <selectionWeight>0.0125</selectionWeight> -->
	<customEffectDescriptions>
		<li>Restores a lost limb or heals scars every (*DateTime)15-30 days(/DateTime).</li>
	</customEffectDescriptions>
  </GeneDef>

