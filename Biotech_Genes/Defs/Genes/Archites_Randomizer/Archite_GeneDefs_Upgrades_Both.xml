  <GeneDef ParentName="WVC_XenotypesAndGenes_Archites" Name="WVC_XenotypesAndGenes_ArchitesRandomEncode" Abstract="True">
	<!-- <geneClass>WVC_XenotypesAndGenes.Gene_WithConditions</geneClass> -->
	<!-- <displayCategory>WVC_MechaBodyWare</displayCategory> -->
	<!-- <label>AI</label> -->
	<!-- <description>Complex templates for the mechanoid sub-core, which can provide some advantages.</description> -->
	<description>Carriers of this gene have formed special neural connections and improved overall physical condition.</description>
	<iconPath>WVC/UI/Genes/Gene_RandomBuff_v0</iconPath>
	<!-- <displayCategory>WVC_MechaBodyWare</displayCategory> -->
	<!-- <displayCategory>WVC_MechaBodyParts</displayCategory> -->
	<displayOrderInCategory>-1200</displayOrderInCategory>
	<!-- <prerequisite>WVC_MechaAI_Base</prerequisite> -->
	<geneClass>WVC_XenotypesAndGenes.Gene_Shuffle</geneClass>
	<randomChosen>true</randomChosen>
	<selectionWeight>0.0</selectionWeight>
	<canGenerateInGeneSet>false</canGenerateInGeneSet>
	<!-- <selectionWeight>0.0075</selectionWeight> -->
	<!-- They cost nothing, as they can only be obtained through a randomizer. -->
	<biostatCpx>0</biostatCpx>
	<biostatArc>0</biostatArc>
	<biostatMet>0</biostatMet>
	<customEffectDescriptions>
		<li>Has a 20% chance of being replaced by any other gene of the same type. The effect is triggered when a gene is transferred/reimplanted/added.</li>
	</customEffectDescriptions>
	<!-- <customEffectDescriptions> -->
		<!-- <li>&lt;color=#ff8585&gt;Activation of the gene requires either the "mecha-subcore" gene or any of the mechaskins.&lt;/color&gt;</li> -->
	<!-- </customEffectDescriptions> -->
	<symbolPack>
	  <prefixSymbols>
		<li><symbol>encod</symbol></li>
		<li><symbol>enco</symbol></li>
		<li><symbol>enc</symbol></li>
	  </prefixSymbols>
	  <suffixSymbols>
		<li><symbol>ed</symbol></li>
		<li><symbol>ded</symbol></li>
	  </suffixSymbols>
	</symbolPack>
	<modExtensions>
		<li Class="VanillaGenesExpanded.GeneExtension">
			<backgroundPathEndogenes>WVC/UI/Genes/GeneBackground_Endogene</backgroundPathEndogenes>
			<backgroundPathXenogenes>WVC/UI/Genes/GeneBackground_Xenogene</backgroundPathXenogenes>
			<hideGene>true</hideGene>
		</li>
	</modExtensions>
  </GeneDef>

