<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef ParentName="WVC_XenotypesAndGenes_GeneRandom_Base" Name="WVC_XenotypesAndGenes_GeneNaturalRandom_WoundHealing" Abstract="True">
	<label>wound healing</label>
	<labelShortAdj>healing</labelShortAdj>
	<description>Carriers of this gene have an unstable wound healing, which can either improve or worsen over generations.</description>
	<iconPath>WVC/UI/Genes/Gene_Random_Healing_v0</iconPath>
	<exclusionTags>
	  <li>WoundHealingRate</li>
	</exclusionTags>
	<modExtensions>
		<li Class="WVC_XenotypesAndGenes.GeneExtension_Giver">
			<randomizerGenesList>
				<li>WVC_RandomWoundHealing_A</li>
				<li>WVC_RandomWoundHealing_B</li>
				<li>WVC_RandomWoundHealing_C</li>
				<li>WVC_RandomWoundHealing_D</li>
				<li>WVC_RandomWoundHealing_E</li>
			</randomizerGenesList>
		</li>
	</modExtensions>
  </GeneDef>

  <!-- ============================= -->

  <GeneDef ParentName="WVC_XenotypesAndGenes_GeneNaturalRandom_WoundHealing">
	<defName>WVC_RandomWoundHealing_A</defName>
	<iconColor>(126, 61, 60)</iconColor>
	<statFactors>
	  <InjuryHealingFactor>0.2</InjuryHealingFactor>
	</statFactors>
