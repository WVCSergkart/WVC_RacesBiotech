<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef Name="WVC_XenotypesAndGenes_GeneRandom_Base" Abstract="True">
	<iconColor>(255, 255, 255)</iconColor>
	<displayCategory>Miscellaneous</displayCategory>
	<displayOrderInCategory>-537</displayOrderInCategory>
	<geneClass>WVC_XenotypesAndGenes.Gene_Shuffle</geneClass>
	<randomChosen>false</randomChosen>
	<selectionWeight>0.0</selectionWeight>
	<canGenerateInGeneSet>false</canGenerateInGeneSet>
	<biostatCpx>0</biostatCpx>
	<biostatArc>0</biostatArc>
	<biostatMet>0</biostatMet>
	<customEffectDescriptions>
		<li>Has a 20% chance of being replaced by any other gene of the same type. The effect is triggered when a gene is transferred/reimplanted/added.</li>
	</customEffectDescriptions>
	<modExtensions>
		<li Class="VanillaGenesExpanded.GeneExtension">
			<backgroundPathEndogenes>WVC/UI/Genes/GeneBackground_Endogene</backgroundPathEndogenes>
			<backgroundPathXenogenes>WVC/UI/Genes/GeneBackground_Xenogene</backgroundPathXenogenes>
			<hideGene>true</hideGene>
		</li>
		<li Class="WVC_XenotypesAndGenes.GeneExtension_Background">
			<backgroundPathArchite>WVC/UI/Genes/GeneBackground_ArchiteGene</backgroundPathArchite>
		</li>
	</modExtensions>
  </GeneDef>

