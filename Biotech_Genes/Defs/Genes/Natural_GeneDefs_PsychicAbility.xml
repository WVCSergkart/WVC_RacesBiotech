	<!-- <exclusionTags> -->
	  <!-- <li>PsychicAbility</li> -->
	<!-- </exclusionTags> -->
	<selectionWeight>0.025</selectionWeight>
	<suppressedTraits>
	  <li>
		<def>PsychicSensitivity</def>
