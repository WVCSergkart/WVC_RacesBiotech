<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Serum -->
  <GeneDef ParentName="WVC_Mechanoidization_GenepackSpawnerBase">
	<defName>WVC_XenotypeSerumSpawner_Random</defName>
	<label>serum thrower</label>
	<labelShortAdj>serum thrower</labelShortAdj>
	<iconPath>WVC/Things/XenotypeSerum_Ultra/XenotypeSerum_Ultra</iconPath>
	<biostatArc>2</biostatArc>
	<customEffectDescriptions Inherit="False">
		<li>Spawns xenotype serum every (*DateTime)15-45 days(/DateTime).</li>
		<li>Xenotype serum (random) - a one-time serum that changes a pawn's xenotype to any random one. Custom xenotypes are not supported.</li>
	</customEffectDescriptions>
	<symbolPack>
	  <prefixSymbols>
		<li><symbol>serum-</symbol></li>
		<li><symbol>serum</symbol></li>
	  </prefixSymbols>
	</symbolPack>
	<modExtensions>
		<li Class="WVC.GeneExtension_Spawner">
			<thingDefToSpawn>WVC_XenotypeSerum_Random</thingDefToSpawn>
			<stackCount>1</stackCount>
			<spawnIntervalRange><min>900000</min><max>2700000</max></spawnIntervalRange>
		</li>
	</modExtensions>
  </GeneDef>

  <GeneDef ParentName="WVC_Mechanoidization_GenepackSpawnerBase">
	<defName>WVC_XenotypeSerumSpawner_HybridRandom</defName>
	<label>hybrid serum thrower</label>
	<labelShortAdj>serum thrower</labelShortAdj>
	<iconPath>WVC/Things/XenotypeSerum_Mecha/XenotypeSerum_Variants_v0</iconPath>
	<biostatArc>2</biostatArc>
	<customEffectDescriptions Inherit="False">
		<li>Spawns hybrid serum every (*DateTime)15-45 days(/DateTime).</li>
		<li>Hybrid serum (random) - a single use serum that gives the pawn two random xenotypes, endo and xeno. Custom xenotypes are not supported.</li>
	</customEffectDescriptions>
	<symbolPack>
	  <prefixSymbols>
		<li><symbol>serum-</symbol></li>
		<li><symbol>serum</symbol></li>
	  </prefixSymbols>
	</symbolPack>
	<modExtensions>
		<li Class="WVC.GeneExtension_Spawner">
			<thingDefToSpawn>WVC_XenotypeSerum_HybridRandom</thingDefToSpawn>
			<stackCount>1</stackCount>
			<spawnIntervalRange><min>900000</min><max>2700000</max></spawnIntervalRange>
		</li>
	</modExtensions>
  </GeneDef>

