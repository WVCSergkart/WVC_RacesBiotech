  <GeneDef Name="WVC_Aptitudes_ZeroHidden" Abstract="True">
	<!-- <defName>WVC_Natural_Base</defName> -->
	<label>skills</label>
	<description>The carrier's aptitude is 0. Aptitude acts like an offset on skill level.</description>
	<iconPath>WVC/UI/Genes/SkillUp_Equal</iconPath>
	<displayOrderInCategory>-990</displayOrderInCategory>
	<!-- <prerequisite>WVC_Natural_Base</prerequisite> -->
	<displayCategory>Aptitudes</displayCategory>
	<biostatCpx>0</biostatCpx>
	<biostatMet>0</biostatMet>
	<biostatArc>0</biostatArc>
	<customEffectDescriptions>
		<li>Nullifying aptitude of the same name.</li>
	</customEffectDescriptions>
	<!-- <aptitudes> -->
	  <!-- <Intellectual>1</Intellectual> -->
	<!-- </aptitudes> -->
	<exclusionTags>
	  <!-- <li>WVC_Mechanoidization_Skills</li> -->
