	<labelShortAdj>runeskin</labelShortAdj>
	<description>TEST DESC</description>
	<displayCategory>WVC_MechaBody</displayCategory>
	<selectionWeight>0.0125</selectionWeight>
	<!-- <marketValueFactor>1.5</marketValueFactor> -->
