<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Soft -->

  <GeneDef ParentName="WVC_MechanoidizationBase" Name="WVC_Mechanoidization_SoftwareBase" Abstract="True">
	<!-- <geneClass>WVC.Gene_WithConditions</geneClass> -->
	<displayCategory>WVC_MechaBodyWare</displayCategory>
	<label>AI</label>
	<description>The carriers of these archites have software that helps them.</description>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareUp</iconPath>
	<displayOrderInCategory>7</displayOrderInCategory>
	<prerequisite>WVC_MechaAI_Base</prerequisite>
	<biostatCpx>0</biostatCpx>
	<biostatArc>1</biostatArc>
	<marketValueFactor>2</marketValueFactor>
	<!-- <customEffectDescriptions> -->
		<!-- <li>&lt;color=#ff8585&gt;Activation of the gene requires either the "mecha-subcore" gene or any of the mechaskins.&lt;/color&gt;</li> -->
	<!-- </customEffectDescriptions> -->
	<symbolPack>
	  <prefixSymbols>
		<li><symbol>ware</symbol></li>
		<li><symbol>soft</symbol></li>
	  </prefixSymbols>
	  <suffixSymbols>
		<li><symbol>ware</symbol></li>
		<li><symbol>soft</symbol></li>
	  </suffixSymbols>
	</symbolPack>
	<!-- <modExtensions> -->
		<!-- <li Class="WVC.GeneExtension_Conditions"> -->
			<!-- <pawnShouldBeMechalike>true</pawnShouldBeMechalike> -->
		<!-- </li> -->
	<!-- </modExtensions> -->
  </GeneDef>

  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MechaAI_SoftwareManipulation</defName>
	<label>fine motor optimization</label>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareUp</iconPath>
	<iconColor>(255, 148, 127)</iconColor>
	<!-- <customEffectDescriptions> -->
		<!-- <li>&lt;color=#b8c6ff&gt;Increases the chance of arrest.&lt;/color&gt;</li> -->
	<!-- </customEffectDescriptions> -->
	<capMods>
	  <li>
		<capacity>Manipulation</capacity>
		<offset>0.1</offset>
	  </li>
	</capMods>
	<exclusionTags>
	  <li>WVC_MechaAI_SoftwareManipulation</li>
	</exclusionTags>
	<!-- <modExtensions Inherit="False"> -->
		<!-- <li Class="WVC.GeneExtension"> -->
			<!-- <backgroundPathArchite>WVC/UI/Genes/GeneBackground_NodeGeneActive</backgroundPathArchite> -->
		<!-- </li> -->
	<!-- </modExtensions> -->
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MechaAI_SoftwareMoving</defName>
	<label>improved pathfinding</label>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareUp</iconPath>
	<iconColor>(187, 255, 127)</iconColor>
	<capMods>
	  <li>
		<capacity>Moving</capacity>
		<offset>0.1</offset>
	  </li>
	</capMods>
	<exclusionTags>
	  <li>WVC_MechaAI_SoftwareMoving</li>
	</exclusionTags>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MecaBodyParts_Arm</defName>
	<label>fight program</label>
	<iconColor>(255, 200, 130)</iconColor>
	<!-- <customEffectDescriptions> -->
		<!-- <li>&lt;color=#b8c6ff&gt;Increases the chance of arrest.&lt;/color&gt;</li> -->
	<!-- </customEffectDescriptions> -->
	<statOffsets>
	  <MeleeDamageFactor>0.15</MeleeDamageFactor>
	</statOffsets>
	<!-- <modExtensions Inherit="False"> -->
		<!-- <li Class="WVC.GeneExtension"> -->
			<!-- <backgroundPathArchite>WVC/UI/Genes/GeneBackground_NodeGeneActive</backgroundPathArchite> -->
		<!-- </li> -->
	<!-- </modExtensions> -->
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MecaBodyParts_Jaw</defName>
	<label>speaking improver</label>
	<iconColor>(220, 130, 255)</iconColor>
	<suppressedTraits>
	  <li>
		<def>AnnoyingVoice</def>
	  </li>
	</suppressedTraits>
	<capMods>
	  <li>
		<capacity>Talking</capacity>
		<offset>0.2</offset>
	  </li>
	</capMods>
	<exclusionTags>
	  <li>WVC_MechaAI_SoftwareDisableTalking</li>
	</exclusionTags>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MecaBodyParts_Nose</defName>
	<label>chemical analyzer</label>
	<iconColor>(130, 150, 255)</iconColor>
	<statFactors>
	  <FoodPoisonChance>0.0</FoodPoisonChance>
	</statFactors>
	<statOffsets>
	  <CookSpeed>1.0</CookSpeed>
	  <WorkSpeedGlobal>0.05</WorkSpeedGlobal>
	</statOffsets>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MecaBodyParts_Ear</defName>
	<label>sound analyzer</label>
	<iconColor>(150, 130, 255)</iconColor>
	<statOffsets>
	  <WorkSpeedGlobal>0.1</WorkSpeedGlobal>
	</statOffsets>
	<capMods>
	  <li>
		<capacity>Hearing</capacity>
		<offset>0.1</offset>
	  </li>
	</capMods>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MecaBodyParts_Eye</defName>
	<label>visual analyzer</label>
	<ignoreDarkness>true</ignoreDarkness>
	<iconColor>(150, 190, 255)</iconColor>
	<statOffsets>
	  <WorkSpeedGlobal>0.1</WorkSpeedGlobal>
	</statOffsets>
	<capMods>
	  <li>
		<capacity>Sight</capacity>
		<offset>0.1</offset>
	  </li>
	</capMods>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MechaAI_MechanitorNetwork</defName>
	<label>mecha-network</label>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareUp</iconPath>
	<iconColor>(170, 170, 255)</iconColor>
	<!-- <biostatArc>1</biostatArc> -->
	<!-- <statOffsets> -->
	  <!-- <WVC_PawnMechanoidization>6</WVC_PawnMechanoidization> -->
	<!-- </statOffsets> -->
	<conditionalStatAffecters>
	  <li Class="WVC.ConditionalStatAffecter_MechanitorUsedBandwidth">
		<usedBandwidth>8</usedBandwidth>
		<statOffsets>
			<MechRepairSpeed>0.5</MechRepairSpeed>
		</statOffsets>
	  </li>
	  <li Class="WVC.ConditionalStatAffecter_MechanitorUsedBandwidth">
		<usedBandwidth>12</usedBandwidth>
		<statOffsets>
			<MechControlGroups>2</MechControlGroups>
		</statOffsets>
	  </li>
	  <li Class="WVC.ConditionalStatAffecter_MechanitorUsedBandwidth">
		<usedBandwidth>16</usedBandwidth>
		<statOffsets>
			<WorkSpeedGlobalOffsetMech>0.5</WorkSpeedGlobalOffsetMech>
		</statOffsets>
	  </li>
	  <li Class="WVC.ConditionalStatAffecter_MechanitorUsedBandwidth">
		<usedBandwidth>20</usedBandwidth>
		<statOffsets>
			<MechBandwidth>12</MechBandwidth>
		</statOffsets>
	  </li>
	  <li Class="WVC.ConditionalStatAffecter_MechanitorUsedBandwidth">
		<usedBandwidth>24</usedBandwidth>
		<statOffsets>
			<MechFormingSpeed>0.5</MechFormingSpeed>
		</statOffsets>
	  </li>
	</conditionalStatAffecters>
	<exclusionTags>
	  <li>WVC_MechanitorNetwork</li>
	</exclusionTags>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MechaAI_SoftwareDisableTalking</defName>
	<label>silent program</label>
	<description>Archits disable the voice functions of the mech, which saves some resources.</description>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareDown</iconPath>
	<iconColor>(150, 70, 70)</iconColor>
	<biostatMet>1</biostatMet>
	<!-- <biostatArc>1</biostatArc> -->
	<!-- <statOffsets> -->
	  <!-- <WVC_PawnMechanoidization>2</WVC_PawnMechanoidization> -->
	<!-- </statOffsets> -->
	<capMods>
	  <li>
		<capacity>Talking</capacity>
		<setMax>0.0</setMax>
	  </li>
	</capMods>
	<exclusionTags>
	  <li>WVC_MechaAI_SoftwareDisableTalking</li>
	</exclusionTags>
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MechaAI_SoftwareFriendOrFoe</defName>
	<label>friend or foe</label>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareUp</iconPath>
	<iconColor>(230, 255, 150)</iconColor>
	<customEffectDescriptions>
		<!-- <li>&lt;color=#b8c6ff&gt;Reduces trade price improvement and bond chance.&lt;/color&gt;</li> -->
		<li>Ancient mechanoids do not attack a pawn, even if a pawn attacks them.</li>
	</customEffectDescriptions>
	<forcedTraits>
	  <li>
		<def>WVC_RB_FriendOrFoeTrait</def>
	  </li>
	</forcedTraits>
	<!-- <modExtensions Inherit="False"> -->
		<!-- <li Class="WVC.GeneExtension"> -->
			<!-- <backgroundPathArchite>WVC/UI/Genes/GeneBackground_NodeGeneActive</backgroundPathArchite> -->
		<!-- </li> -->
	<!-- </modExtensions> -->
  </GeneDef>
  <!-- = -->
  <GeneDef ParentName="WVC_Mechanoidization_SoftwareBase">
	<defName>WVC_MechaAI_SoftwareNaturalEnemy</defName>
	<label>mecha-minded</label>
	<iconPath>WVC/UI/Genes/MechanoidizationSoftwareDown</iconPath>
	<iconColor>(150, 70, 150)</iconColor>
	<biostatCpx>1</biostatCpx>
	<!-- <biostatMet>3</biostatMet> -->
	<!-- <biostatArc>2</biostatArc> -->
	<statFactors>
	  <TradePriceImprovement>0.2</TradePriceImprovement>
	  <BondAnimalChanceFactor>0.0</BondAnimalChanceFactor>
	</statFactors>
	<customEffectDescriptions>
		<!-- <li>&lt;color=#b8c6ff&gt;Reduces trade price improvement and bond chance.&lt;/color&gt;</li> -->
		<li>Increases the opinion of pawns with the same gene by 20. (Mechaskin only)</li>
		<li>The opinion of all pawns that do not have the mechaskin gene is reduced by 500. (Mechaskin only)</li>
		<!-- <li>Ancient mechanoids do not attack a pawn, even if a pawn attacks them.</li> -->
	</customEffectDescriptions>
	<!-- <modExtensions Inherit="False"> -->
		<!-- <li Class="WVC.GeneExtension"> -->
			<!-- <backgroundPathArchite>WVC/UI/Genes/GeneBackground_NodeGeneActive</backgroundPathArchite> -->
		<!-- </li> -->
	<!-- </modExtensions> -->
  </GeneDef>

</Defs>