  <!-- Forever young -->

  <GeneDef>
	<defName>WVC_ForeverYoung</defName>
	<label>forever young</label>
	<description>Carriers of this gene have malfunctioning architis that prevents them from growing naturally.\n\n&lt;color=#f5ffa2&gt;Note:&lt;/color&gt; Pawns cannot become adults, but this only applies to children who grow up in a colony. During generation, the age of pawns with this gene can be any.</description>
	<iconPath>WVC/UI/Genes/AgeDebuff_ForeverYoung</iconPath>
	<displayCategory>WVC_AgeDebuffs</displayCategory>
	<displayOrderInCategory>100</displayOrderInCategory>
	<minAgeActive>10</minAgeActive>
	<biologicalAgeTickFactorFromAgeCurve>
	  <points>
		<li>(11, 1)</li>
		<li>(12, 0)</li>
		<li>(13, 0)</li>
		<!-- <li>(14, 1)</li> -->
		<li>(15, 1)</li>
	  </points>
	</biologicalAgeTickFactorFromAgeCurve>
	<biostatCpx>6</biostatCpx>
	<biostatArc>1</biostatArc>
	<biostatMet>6</biostatMet>
	<symbolPack>
	  <wholeNameSymbols>
		<li><symbol>young</symbol></li>
	  </wholeNameSymbols>
	  <prefixSymbols>
		<li><symbol>for</symbol></li>
		<li><symbol>fore</symbol></li>
		<li><symbol>eve</symbol></li>
		<li><symbol>ever</symbol></li>
	  </prefixSymbols>
	  <suffixSymbols>
		<li><symbol>you</symbol></li>
		<li><symbol>yo</symbol></li>
	  </suffixSymbols>
	</symbolPack>
