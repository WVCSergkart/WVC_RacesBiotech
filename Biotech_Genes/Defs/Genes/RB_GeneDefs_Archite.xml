<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- Gray goo -->

  <GeneDef>
	<defName>WVC_GrayGooSkin</defName>
	<label>gray goo skin</label>
	<description>Carriers of this gene have archites which replace the skin making it soft and very unpleasant to look at and touch. However, due to such changes, archites absorb almost all incoming damage, making the owners of this gene practically invulnerable.</description>
	<iconPath>WVC/UI/Genes/NoArmor</iconPath>
	<displayCategory>Archite</displayCategory>
	<displayOrderInCategory>150</displayOrderInCategory>
	<iconColor>(79, 80, 81)</iconColor>
	<!-- <skinColorOverride>(79, 80, 81)</skinColorOverride> -->
	<!-- <hairColorOverride>(79, 80, 81)</hairColorOverride> -->
	<missingGeneRomanceChanceFactor>0.2</missingGeneRomanceChanceFactor>
	<statOffsets>
	  <!-- <ArmorRating_Blunt>-5.0</ArmorRating_Blunt> -->
	  <!-- <ArmorRating_Sharp>-5.0</ArmorRating_Sharp> -->
	  <!-- <ArmorRating_Heat>-5.0</ArmorRating_Heat> -->
	  <!-- <PawnBeauty>-2</PawnBeauty> -->
	</statOffsets>
	<statFactors>
	  <!-- <ArmorRating_Blunt>0</ArmorRating_Blunt> -->
	  <!-- <ArmorRating_Sharp>0</ArmorRating_Sharp> -->
	  <!-- <ArmorRating_Heat>0</ArmorRating_Heat> -->
	  <IncomingDamageFactor>0.3</IncomingDamageFactor>
	</statFactors>
	<biostatCpx>6</biostatCpx>
	<!-- <biostatMet>-2</biostatMet> -->
	<biostatArc>2</biostatArc>
	<exclusionTags>
	  <!-- <li>WVC_NaturalArmor</li> -->
	  <!-- <li>Toughness</li> -->
	  <!-- <li>Beauty</li> -->
	  <!-- <li>Fur</li> -->
	  <!-- <li>HairColor</li> -->
	  <!-- <li>SkinColorOverride</li> -->
	  <!-- <li>SkinColor</li> -->
