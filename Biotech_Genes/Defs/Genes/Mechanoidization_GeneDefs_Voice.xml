<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <GeneDef ParentName="GeneVoiceBase" Name="WVC_MechaVoicesBase" Abstract="True">
	<!-- <displayCategory>WVC_MechaBody</displayCategory> -->
	<displayCategory>WVC_MechaBodyVoices</displayCategory>
	<description>Carriers of this gene have archites that change their voice to a cool mechanoid voice.</description>
	<displayOrderInCategory>300</displayOrderInCategory>
	<iconPath>WVC/UI/Genes/MechanoidizationVoice</iconPath>
	<iconColor>(255, 255, 255)</iconColor>
	<!-- <prerequisite>WVC_MechaSkin</prerequisite> -->
	<!-- <prerequisite>WVC_MechaAI_Base</prerequisite> -->
	<biostatArc>1</biostatArc>
	<minAgeActive>13</minAgeActive>
	<biostatCpx>0</biostatCpx>
	<randomChosen>true</randomChosen>
	<canGenerateInGeneSet>true</canGenerateInGeneSet>
	<selectionWeight>0.15</selectionWeight>
	<!-- <statOffsets> -->
	  <!-- <WVC_PawnMechanoidization>2</WVC_PawnMechanoidization> -->
	<!-- </statOffsets> -->
	<symbolPack>
	  <wholeNameSymbols>
		<li><symbol>mechanoid</symbol></li>
	  </wholeNameSymbols>
	  <prefixSymbols>
		<li><symbol>mecha</symbol></li>
	  </prefixSymbols>
	  <suffixSymbols>
		<li><symbol>noid</symbol></li>
	  </suffixSymbols>
	</symbolPack>
	<suppressedTraits>
	  <li>
		<def>AnnoyingVoice</def>
	  </li>
	</suppressedTraits>
	<modExtensions>
		<!-- <li Class="VanillaGenesExpanded.GeneExtension"> -->
			<!-- <backgroundPathEndogenes>WVC/UI/Genes/GeneBackground_ColorlessGene</backgroundPathEndogenes> -->
			<!-- <backgroundPathXenogenes>WVC/UI/Genes/GeneBackground_ColorlessGene</backgroundPathXenogenes> -->
		<!-- </li> -->
		<li Class="WVC.GeneExtension">
			<backgroundPathArchite>WVC/UI/Genes/GeneBackground_NodeGene</backgroundPathArchite>
		</li>
	</modExtensions>
  </GeneDef>

