<Defs>

  <!-- Aptitude -->

  <GeneTemplateDef ParentName="GeneAptitudeBase">
	<defName>WVC_AptitudeImpossible</defName>
	<label>impossible {0}</label>
	<description>The carrier's aptitude in {0} is reduced by 20. Aptitude acts like an offset on skill level. Additionally, all passion is removed from {0}.</description>
	<!-- <iconPath>UI/Icons/Genes/Skills/{0}/Terrible</iconPath> -->
	<iconPath>WVC/UI/Genes/SkillDown</iconPath>
	<aptitudeOffset>-20</aptitudeOffset>
	<passionModType>DropAll</passionModType>
	<displayOrderOffset>-100</displayOrderOffset>
	<biostatCpx>1</biostatCpx>
	<biostatMet>8</biostatMet>
	<!-- <modExtensions> -->
		<!-- <li Class="VanillaGenesExpanded.GeneExtension"> -->
			<!-- <backgroundPathEndogenes>WVC/UI/Genes/GeneBackground_ArchiteGene</backgroundPathEndogenes> -->
			<!-- <backgroundPathXenogenes>WVC/UI/Genes/GeneBackground_ArchiteGene</backgroundPathXenogenes> -->
		<!-- </li> -->
	<!-- </modExtensions> -->
  </GeneTemplateDef>

  <GeneTemplateDef ParentName="GeneAptitudeBase">
	<defName>WVC_AptitudePerfect</defName>
	<label>perfect {0}</label>
	<description>The carrier's aptitude in {0} is increased by 20. Aptitude acts like an offset on skill level. Additionally, one level of passion is added to {0}.</description>
	<!-- <iconPath>UI/Icons/Genes/Skills/{0}/Remarkable</iconPath> -->
	<iconPath>WVC/UI/Genes/SkillUp</iconPath>
	<aptitudeOffset>20</aptitudeOffset>
	<passionModType>AddOneLevel</passionModType>
	<displayOrderOffset>100</displayOrderOffset>
	<biostatCpx>5</biostatCpx>
	<biostatMet>-12</biostatMet>
	<!-- <modExtensions> -->
