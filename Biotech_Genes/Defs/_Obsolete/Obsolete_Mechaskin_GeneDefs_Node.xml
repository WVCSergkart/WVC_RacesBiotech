  <!-- Bodies -->

  <GeneDef ParentName="WVC_MechanoidBody_Base" Name="WVC_Obsolete_NodeBody_Base" Abstract="True" WVC_Tag="MechaAndNodeSkins_EasyPatch">
	<labelShortAdj>nodeskin</labelShortAdj>
	<description>&lt;color=#ff8585&gt;Obsolete.&lt;/color&gt;</description>
	<randomChosen>true</randomChosen>
	<displayOrderInCategory>-95</displayOrderInCategory>
	<iconColor>(255, 255, 255)</iconColor>
	<selectionWeight>0</selectionWeight>
	<canGenerateInGeneSet>false</canGenerateInGeneSet>
	<biostatMet>0</biostatMet>
	<graphicData>
	  <fur>WVC_NodeSkin</fur>
	  <!-- <tattoosVisible>false</tattoosVisible> -->
	  <tattoosVisible>true</tattoosVisible>
	  <colorType>Custom</colorType>
	  <color>(255, 255, 255)</color>
	  <layer>PostTattoo</layer>
	  <!-- <drawOnEyes>true</drawOnEyes> -->
	  <!-- <layer>PostSkin</layer> -->
	  <useSkinShader>false</useSkinShader>
	  <drawIfFaceCovered>true</drawIfFaceCovered>
	</graphicData>
	<exclusionTags>
	  <li>WVC_NodeBody</li>
	</exclusionTags>
	<beardTagFilter>
	  <whitelist>true</whitelist>
	  <tags>
		<li>WVC_NodeSkin</li>
	  </tags>
	</beardTagFilter>
	<symbolPack>
	  <wholeNameSymbols>
		<li><symbol>former</symbol></li>
		<li><symbol>formkind</symbol></li>
		<li><symbol>nodekind</symbol></li>
	  </wholeNameSymbols>
	  <prefixSymbols>
		<li><symbol>node</symbol></li>
		<li><symbol>nod</symbol></li>
		<li><symbol>no</symbol></li>
		<li><symbol>form</symbol></li>
		<li><symbol>for</symbol></li>
		<li><symbol>fo</symbol></li>
	  </prefixSymbols>
	  <suffixSymbols>
		<li><symbol>er</symbol></li>
		<li><symbol>nod</symbol></li>
		<li><symbol>d</symbol></li>
	  </suffixSymbols>
	</symbolPack>
	<modExtensions Inherit="False">
		<li Class="VanillaGenesExpanded.GeneExtension">
			<hideGene>true</hideGene>
			<furHidesBody>True</furHidesBody>
			<useSkinColorForFur>True</useSkinColorForFur>
			<customWoundsFromFleshtype>Mechanoid</customWoundsFromFleshtype>
			<customBloodEffect>Damage_HitMechanoid</customBloodEffect>
		</li>
		<li Class="WVC.GeneExtension_Background">
			<backgroundPathArchite>WVC/UI/Genes/GeneBackground_ArchiteGene</backgroundPathArchite>
		</li>
		<li Class="WVC.GeneExtension_General">
			<eyesShouldBeInvisble>true</eyesShouldBeInvisble>
		</li>
	</modExtensions>
  </GeneDef>

