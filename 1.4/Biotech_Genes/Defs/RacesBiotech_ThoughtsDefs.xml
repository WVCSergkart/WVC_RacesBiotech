<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThoughtDef>
	<defName>WVC_IngestedNutrientsInjector</defName>
	<thoughtClass>Thought_Memory</thoughtClass>
	<durationDays>3</durationDays>
	<stackLimit>5</stackLimit>
	<nullifyingTraits>
	  <li>Transhumanist</li>
	</nullifyingTraits>
	<nullifyingGenes>
	  <li>WVC_Dustogenic</li>
	</nullifyingGenes>
	<nullifyingPrecepts>
	  <li MayRequire="Ludeon.RimWorld.Ideology">NutrientPasteEating_DontMind</li>
	</nullifyingPrecepts>
	<stages>
	  <li>
		<label>ingested dust food</label>
		<description>I ingested a dustogenic food. It may be nutritious, but it is completely tasteless.</description>
		<baseMoodEffect>-4</baseMoodEffect>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_IngestedUltranites</defName>
	<thoughtClass>Thought_Memory</thoughtClass>
	<durationDays>3</durationDays>
	<stackLimit>10</stackLimit>
	<stages>
	  <li>
		<label>ingested ultranites</label>
		<description>I ingested a ultranites. I still can't forget that indescribably terrible taste and feeling.</description>
		<baseMoodEffect>-1</baseMoodEffect>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_MechaAI_MechaSensesFriendOrFoe</defName>
	<thoughtClass>Thought_SituationalSocial</thoughtClass>
	<workerClass>WVC_XenotypesAndGenes.ThoughtWorker_MechaSensesFriendOrFoe</workerClass>
	<validWhileDespawned>true</validWhileDespawned>
	<requiredGenes>
	  <li>WVC_MechaAI_SoftwareNaturalEnemy</li>
	</requiredGenes>
	<stages>
	  <li>
		<label>same as me</label>
		<baseOpinionOffset>20</baseOpinionOffset>
	  </li>
	  <li>
		<label>not like me</label>
		<baseOpinionOffset>-500</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_Dustogenic_AngelBeauty</defName>
	<thoughtClass>Thought_SituationalSocial</thoughtClass>
	<workerClass>WVC_XenotypesAndGenes.ThoughtWorker_AngelBeauty</workerClass>
	<validWhileDespawned>true</validWhileDespawned>
	<nullifyingGenes>
	  <li>WVC_MechaAI_SoftwareNaturalEnemy</li>
	  <li>WVC_MechaAI_PsychicDull</li>
	</nullifyingGenes>
	<stages>
	  <li>
		<label>angel</label>
		<baseOpinionOffset>500</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_Social_SameBloodSameXenotype</defName>
	<thoughtClass>Thought_SituationalSocial</thoughtClass>
	<workerClass>WVC_XenotypesAndGenes.ThoughtWorker_SameBloodSameXenotype</workerClass>
	<validWhileDespawned>true</validWhileDespawned>
	<requiredGenes>
	  <li>WVC_SameBloodSameXenotype</li>
	</requiredGenes>
	<stages>
	  <li>
		<label>other xenotype</label>
		<visible>false</visible>
		<baseOpinionOffset>0</baseOpinionOffset>
	  </li>
	  <li>
		<label>same xenotype</label>
		<baseOpinionOffset>30</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_Social_GeneFamilyByBlood_Basic</defName>
	<thoughtClass>Thought_SituationalSocial</thoughtClass>
	<workerClass>WVC_XenotypesAndGenes.ThoughtWorker_FamilyByBlood</workerClass>
	<validWhileDespawned>true</validWhileDespawned>
	<requiredGenes>
	  <li>WVC_FamilyByBlood_Basic</li>
	</requiredGenes>
	<stages>
	  <li>
		<label>family by blood</label>
		<baseOpinionOffset>20</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_Social_GeneFamilyByBlood_Dustogenic</defName>
	<thoughtClass>Thought_SituationalSocial</thoughtClass>
	<workerClass>WVC_XenotypesAndGenes.ThoughtWorker_FamilyByBlood</workerClass>
	<validWhileDespawned>true</validWhileDespawned>
	<requiredGenes>
	  <li>WVC_FamilyByBlood_Dustogenic</li>
	</requiredGenes>
	<stages>
	  <li>
		<label>family by blood</label>
		<baseOpinionOffset>100</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <!-- ========================================================== -->

  <ThoughtDef>
	<defName>WVC_XenotypesAndGenes_ResurrectedMe</defName>
	<thoughtClass>Thought_MemorySocial</thoughtClass>
	<durationDays>20</durationDays>
	<stackLimit>5</stackLimit>
	<stackLimitForSameOtherPawn>10</stackLimitForSameOtherPawn>
	<stackedEffectMultiplier>0.9</stackedEffectMultiplier>
	<developmentalStageFilter>Baby, Child, Adult</developmentalStageFilter>
	<socialTargetDevelopmentalStageFilter>Baby, Child, Adult</socialTargetDevelopmentalStageFilter>
	<nullifyingTraits>
	  <li>Psychopath</li>
	</nullifyingTraits>
	<stages>
	  <li>
		<label>resurrected me</label>
		<baseOpinionOffset>15</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_XenotypesAndGenes_ResurrectedByMe</defName>
	<thoughtClass>Thought_MemorySocial</thoughtClass>
	<durationDays>20</durationDays>
	<stackLimit>5</stackLimit>
	<stackLimitForSameOtherPawn>10</stackLimitForSameOtherPawn>
	<stackedEffectMultiplier>0.9</stackedEffectMultiplier>
	<developmentalStageFilter>Baby, Child, Adult</developmentalStageFilter>
	<socialTargetDevelopmentalStageFilter>Baby, Child, Adult</socialTargetDevelopmentalStageFilter>
	<nullifyingTraits>
	  <li>Psychopath</li>
	</nullifyingTraits>
	<stages>
	  <li>
		<label>resurrected by me</label>
		<baseOpinionOffset>5</baseOpinionOffset>
	  </li>
	</stages>
  </ThoughtDef>

  <ThoughtDef>
	<defName>WVC_XenotypesAndGenes_WasResurrected</defName>
	<durationDays>6</durationDays>
	<stackLimit>1</stackLimit>
	<stages>
	  <li>
		<label>was resurrected</label>
		<description>I got a second chance, this time I won't die so easily.</description>
		<baseMoodEffect>20</baseMoodEffect>
	  </li>
	</stages>
  </ThoughtDef>

