	<biostatCpx>3</biostatCpx>
	<biostatMet>-6</biostatMet>
	<exclusionTags>
	  <li>WVC_DiseaseFree</li>
	</exclusionTags>
	<modExtensions>
		<li Class="WVC_XenotypesAndGenes.GeneExtension_General">
			<inheritableGeneDefs>
			  <li>DiseaseFree</li>
			</inheritableGeneDefs>
		</li>
	</modExtensions>
