<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantFoodRawBase">
	<defName>WVC_RawResurgentFungus</defName>
	<label>raw resurgent fungus</label>
	<description>Raw resurgent fungus. Once harvested, resurgent fungus are very dietarily similar to fungus. However, when eaten raw, they have a higher chance of causing food poisoning.</description>
	<possessionCount>10</possessionCount>
	<graphicData>
	  <texPath>WVC/Things/XaG_Items/RawFood/RawResurgentShroom</texPath>
	</graphicData>
	<statBases>
	  <MarketValue>0.60</MarketValue>
	  <FoodPoisonChanceFixedHuman>0.08</FoodPoisonChanceFixedHuman>
	</statBases>
	<ingestible>
	  <foodType>Fungus</foodType>
	</ingestible>
	<ingredient>
	  <mergeCompatibilityTags>
		<li MayRequire="Ludeon.RimWorld.Ideology">Fungus</li>
	  </mergeCompatibilityTags>
	</ingredient>
	<comps>
	  <li Class="CompProperties_Rottable">
		<daysToRotStart>30</daysToRotStart>
		<rotDestroys>true</rotDestroys>
	  </li>
	</comps>
  </ThingDef>

</Defs>
