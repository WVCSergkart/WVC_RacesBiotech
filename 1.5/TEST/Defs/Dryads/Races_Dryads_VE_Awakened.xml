<Defs>
	<!-- Carrier -->
	<ThingDef MayRequire="vanillaexpanded.ideo.dryads"  ParentName="WVC_XenotypesAndGenes_Dryads_Base">
		<defName>WVC_XaG_VDE_AwakenedDryad_Carrier</defName>
		<label>awakened carrier dryad</label>
		<description>A dryad caste specialized in carrying items. Its awakening has improved their size, carrying capacity and added functionality as a pack animal for long-distance hauling.\n\nAwakened dryads are significantly superior to their bound relatives. Being connected to the owner of a special gene, they have all the strengths of both awakened dryads and ordinary dryads.</description>
		<statBases>
			<MoveSpeed>2.95</MoveSpeed>
		</statBases>
		<race>
			<baseBodySize>2.4</baseBodySize>
			<baseHealthScale>1.75</baseHealthScale>
			<trainability>Advanced</trainability>
			<lifeStageAges>
				<li>
					<def>AnimalAdult</def>
					<minAge>0</minAge>
					<soundWounded>Pawn_Dryad_Wounded</soundWounded>
					<soundCall>Pawn_Dryad_Call</soundCall>
				</li>
			</lifeStageAges>
			<soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
			<soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
			<soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
		</race>
		<tools>
			<li>
				<label>left claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>8</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontLeftPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>right claw</label>
				<capacities>
					<li>Scratch</li>
				</capacities>
				<power>8</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>FrontRightPaw</linkedBodyPartsGroup>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<capacities>
					<li>Bite</li>
				</capacities>
				<power>9</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
				<chanceFactor>0.9</chanceFactor>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>14</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
			<li>
				<label>head</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>4</power>
				<cooldownTime>2</cooldownTime>
				<linkedBodyPartsGroup>HeadAttackTool</linkedBodyPartsGroup>
				<ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
				<chanceFactor>0.2</chanceFactor>
			</li>
		</tools>
	</ThingDef>
	<PawnKindDef MayRequire="vanillaexpanded.ideo.dryads"  ParentName="WVC_XenotypesAndGenes_DryadsKind_Base">
		<defName>WVC_XaG_VDE_AwakenedDryad_Carrier</defName>
		<label>awakened carrier dryad</label>
		<race>WVC_XaG_VDE_AwakenedDryad_Carrier</race>
		<lifeStages>
			<li>
				<bodyGraphicData>
					<texPath>Things/Pawn/Animal/DryadAwakenedCarrier/DryadAwakenedCarrier</texPath>
					<drawSize>3</drawSize>
					<shadowData>
						<volume>(0.8, 0.6, 0.6)</volume>
						<offset>(0,0,-0.3)</offset>
					</shadowData>
				</bodyGraphicData>
				<dessicatedBodyGraphicData>
					<texPath>Things/Pawn/Animal/DryadAwakenedCarrier/Dessicated_DryadAwakenedCarrier</texPath>
					<drawSize>3</drawSize>
				</dessicatedBodyGraphicData>
			</li>
		</lifeStages>
	</PawnKindDef>
	<!-- Clawer -->
	<ThingDef MayRequire="vanillaexpanded.ideo.dryads"  ParentName="WVC_XenotypesAndGenes_Dryads_Base">
