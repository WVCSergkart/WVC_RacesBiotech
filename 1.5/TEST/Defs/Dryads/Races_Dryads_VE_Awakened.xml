			<MoveSpeed>5</MoveSpeed>
		</statBases>
		<comps>
			<li Class="AnimalBehaviours.CompProperties_Floating"/>
		</comps>
		<race>
			<baseBodySize>1.4</baseBodySize>
			<baseHealthScale>1.4</baseHealthScale>
			<trainability>Intermediate</trainability>
			<lifeStageAges>
				<li>
					<def>AnimalAdult</def>
					<minAge>0</minAge>
