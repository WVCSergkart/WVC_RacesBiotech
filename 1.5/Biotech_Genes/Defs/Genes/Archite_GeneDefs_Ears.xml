	<description>Carriers of this gene have cat-like ears.</description>
	<iconPath>WVC/BodyParts/MecaGene_Ears/CatEars_icon</iconPath>
	<biostatCpx>1</biostatCpx>
	<biostatArc>0</biostatArc>
	<renderNodeProperties>
	  <li>
		<nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
		<workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
		<texPath>WVC/BodyParts/MecaGene_Ears/NaturalCatEars</texPath>
		<colorType>Hair</colorType>
		<parentTagDef>Head</parentTagDef>
		<useSkinShader>false</useSkinShader>
		<useRottenColor>true</useRottenColor>
		<rotDrawMode>Fresh, Rotting</rotDrawMode>
		<drawData>
		  <defaultData>
			<layer>70</layer>
		  </defaultData>
		</drawData>
	  </li>
	</renderNodeProperties>
	<capMods>
	  <li>
		<capacity>Hearing</capacity>
		<offset>0.02</offset>
	  </li>
	</capMods>
  </GeneDef>

