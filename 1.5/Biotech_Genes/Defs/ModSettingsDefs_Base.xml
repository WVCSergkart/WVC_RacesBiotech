  <WVC_XenotypesAndGenes.SettingsDef>
	<defName>WVC_UltraPerformance</defName>
	<label>ultra performance</label>
	<description>Settings for maximum performance. In this preset most of the mod mechanics and all graphics are disabled, keep in mind that some genes will stop working. Use this preset only as a last resort, and only if you know what you are doing.</description>
	<presetOrder>100</presetOrder>
	<hideXaGGenes>true</hideXaGGenes>
	<disableFurGraphic>true</disableFurGraphic>
	<disableAllGraphic>true</disableAllGraphic>
	<disableUniqueGeneInterface>true</disableUniqueGeneInterface>
	<generateSkillGenes>false</generateSkillGenes>
	<harmony_EnableGenesMechanicsTriggers>false</harmony_EnableGenesMechanicsTriggers>
	<enableIncestLoverGene>false</enableIncestLoverGene>
	<disableNonAcceptablePreyGenes>true</disableNonAcceptablePreyGenes>
	<enable_xagHumanComponent>false</enable_xagHumanComponent>
	<fixVanillaGeneImmunityCheck>false</fixVanillaGeneImmunityCheck>
	<enable_birthQualityOffsetFromGenes>false</enable_birthQualityOffsetFromGenes>
	<enable_dryadQueenMechanicGenerator>false</enable_dryadQueenMechanicGenerator>
	<enable_StartingFoodPolicies>false</enable_StartingFoodPolicies>
	<enable_OverOverridableGenesMechanic>false</enable_OverOverridableGenesMechanic>
  </WVC_XenotypesAndGenes.SettingsDef>

