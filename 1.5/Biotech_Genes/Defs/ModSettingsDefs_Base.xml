	<enable_ReplaceSimilarGenesAutopatch>false</enable_ReplaceSimilarGenesAutopatch>
	<!-- <bloodfeeder_AutoBloodfeed>True</bloodfeeder_AutoBloodfeed> -->
	<enable_OverOverridableGenesMechanic>true</enable_OverOverridableGenesMechanic>
	<generateSkillGenes>false</generateSkillGenes>
	<!-- <link_addedMechlinkWithGene>false</link_addedMechlinkWithGene> -->
	<!-- <link_addedPsylinkWithGene>false</link_addedPsylinkWithGene> -->
