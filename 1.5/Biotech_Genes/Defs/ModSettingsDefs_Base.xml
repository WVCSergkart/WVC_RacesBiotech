  <WVC_XenotypesAndGenes.SettingsDef>
	<defName>WVC_MyChoice</defName>
	<label>dev choice</label>
	<description>These are the mod developer's preferred settings. Please note that this preset may not suit your playstyle.</description>
	<presetOrder>20</presetOrder>
	<onlyXenotypesMode>true</onlyXenotypesMode>
	<!-- Traders Sell Serums -->
	<spawnXenoForcerSerumsFromTraders>false</spawnXenoForcerSerumsFromTraders>
